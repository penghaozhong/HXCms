<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hx.cms.entity.grab">
	<class name="Bankgold" table="ie_bankgold">
 		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
	 	<property name="variety" column="variety" type="string" not-null="false" length="100"/>
	 	<property name="midpri" column="midpri" type="string" not-null="false" length="100"/>
	 	<property name="buypri" column="buypri" type="string" not-null="false" length="100"/>
	 	<property name="sellpri" column="sellpri" type="string" not-null="false" length="100"/>
	 	<property name="maxpri" column="maxpri" type="string" not-null="false" length="100"/>
	 	<property name="minpri" column="minpri" type="string" not-null="false" length="100"/>
	 	<property name="todayopen" column="todayopen" type="string" not-null="false" length="100"/>
	 	<property name="closeyes" column="closeyes" type="string" not-null="false" length="100"/> 
	 	<property name="quantpri" column="quantpri" type="string" not-null="false" length="100"/> 
	 	<property name="time" column="time" type="string" not-null="false" length="100"/> 
	 	<property name="externaldate" column="externaldate" type="string" not-null="false" length="100" />
	</class>
</hibernate-mapping>