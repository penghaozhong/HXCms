<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hx.cms.entity.grab">
	<class name="CommBank" table="ie_commbank">
 		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment" />
		</id>
		<property name="currency" column="currency" type="string" not-null="false" length="100"/>
		<property name="per" column="per" type="string" not-null="false" length="100"/>
	 	<property name="exchBid" column="exchBid" type="string" not-null="false" length="100"/>
	 	<property name="exchOffer" column="exchOffer" type="string" not-null="false" length="100"/>
	 	<property name="cashBid" column="cashBid" type="string" not-null="false" length="100"/>
	 	<property name="cashOffer" column="cashOffer" type="string" not-null="false" length="100"/>
	 	<property name="timeUpate" column="timeUpate" type="string" not-null="false" length="100"/>
	 	<property name="externaldate" column="externaldate" type="string" not-null="false" length="100" />
	</class>
</hibernate-mapping>